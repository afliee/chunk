{
    "version": 2,
    "builds": [
        {
            "src": "./src/index.js",
            "use": "@now/node"
        },
        {
            "src": "package.json",
            "use": "@now/static-build"
        },
        {
            "src": "package-lock.json",
            "use": "@now/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ]
}